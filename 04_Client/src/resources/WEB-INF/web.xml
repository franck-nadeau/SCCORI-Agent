<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app> 

    <display-name>SCCORI Agents</display-name>
    <description>
        Client for the SCCORI Agent system.
    </description>

    <servlet>
      <servlet-name>ClientServlet</servlet-name>
      <servlet-class>ca.athabascau.sccori.client.ClientServlet</servlet-class>
    </servlet>

    <servlet-mapping>
      <servlet-name>ClientServlet</servlet-name>
      <url-pattern>/Client</url-pattern>
    </servlet-mapping>

</web-app>

